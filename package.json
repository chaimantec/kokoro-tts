{
  "name": "kokoro-tts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && vite build",
    "preview": "vite preview",
    "api:setup": "cd src/api && npm run setup",
    "api:start": "cd src/api && npm start",
    "package-extension": "./package-extension.sh",
    "test:kokoro": "vite --config vite.test.config.js",
    "test:kokoro:node": "node test-kokoro-node.js",
    "test:kokoro:stream": "node test-kokoro-stream.js",
    "test:kokoro:stream:webgpu": "node test-kokoro-stream.js --webgpu"
  },
  "dependencies": {
    "kokoro-js": "^1.2.1",
    "onnxruntime-web": "^1.22.0",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.11.24",
    "@types/webextension-polyfill": "^0.12.3",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-static-copy": "^1.0.1",
    "vite-plugin-web-extension": "^4.4.3"
  }
}
